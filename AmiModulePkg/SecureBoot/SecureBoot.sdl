    TargetH = Yes
    Range    = "Supported options: 3,4,5"
End

TOKEN
    Name  = "SecureBoot_DEBUG_MESSAGES"
    Value  = "0"
    TokenType = Boolean
    Help  = "Control level of debug messages in SecureBoot source code"
    Token = "DEBUG_CODE" "=" "1"
End

TOKEN
