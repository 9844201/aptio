TOKEN
	Name  = "GraphicsConsole_SUPPORT"
	Value  = "1"
	Help  = "Main switch to enable GraphicsConsole support in Project"
	TokenType = Boolean
	TargetEQU = Yes
	TargetMAK = Yes
	Master = Yes
End

TOKEN
	Name  = "GC_COLOR_BLACK"
	Value  = "0,0,0"
	Help  = "Definition of the black color used by the graphic console\Format: <Blue>,<Green>,<Red>"
	TokenType = Expression
	TargetH = Yes
	Range  = "<0-255>,<0-255>,<0-255>"
End

