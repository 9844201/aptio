TOKEN
	Name  = "GC_MODE0"
	Value  = "{ 0, 80, 25, 800, 600 }"
	Help  = "Parameters of the graphical console text mode 0.\NOTE: Text resolution of the mode 0 must be 80 by 25\Format: {ModeNumber, TextColomns,TextRows, HorizontalPixels,VerticalPixels}"
	TokenType = Expression
	TargetH = Yes
End

TOKEN
	Name  = "GC_MODE1"
	Value  = "{ 1, 80, 50, 1280, 1024 }"
	Help  = "Parameters of the graphical console text mode 1.\NOTE: Text resolution of the mode 1 must be 80 by 50\Format: {ModeNumber, TextColomns,TextRows, HorizontalPixels,VerticalPixels}"
	TokenType = Expression
	TargetH = Yes
End

TOKEN
	Name  = "GC_MODE2"
	Value  = "{ 2, 100, 31, 800, 600 }"
	Help  = "Parameters of the graphical console text mode 2.\Format: {ModeNumber, TextColomns,TextRows, HorizontalPixels,VerticalPixels}"
	TokenType = Expression
	TargetH = Yes
End

TOKEN
	Name  = "GC_MODE3"
	Value  = "{ 3, MAX_RES, MAX_RES, MAX_RES, MAX_RES }"
	Help  = "Parameters of the graphical console text mode 3.\This mode uses MAX_RES to signify that the maximum supported resolution should be used, and the maximum number of rows and columns that will fit in that resolution.\Format: {ModeNumber, TextColomns,TextRows, HorizontalPixels,VerticalPixels}"
	TokenType = Expression
	TargetH = Yes
End

TOKEN
	Name  = "GC_MODE_LIST"
	Value  = "$(GC_MODE0), $(GC_MODE1), $(GC_MODE2), $(GC_MODE3)"
	Help  = "List of text modes supported by graphic console.\Format: {ModeNumber, TextColomns,TextRows, HorizontalPixels,VerticalPixels}"
	TokenType = Expression
	TargetH = Yes
End

TOKEN
    Name  = "GC_INITIAL_MODE"
    Value  = "0"
    Help  = "This value is an index into the the GC_MODE_LIST array token that indicates\ the text mode that the system will be in when the system starts"
    TokenType = Integer
    TargetH = Yes
End


INFComponent
	Name  = "GraphicsConsole"
