    TokenType = Boolean
    TargetEQU = Yes
    TargetMAK = Yes
    Master = Yes
End

PATH
    Name  = "TcgPlatformSetupPolicy_DIR"
End

ELINK
	Name  = "AdditionalTcgSetupPolicyPackages"
	InvokeOrder = ReplaceParent
End


