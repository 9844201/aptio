End


FFS_FILE
    Name  = "TcgPlatformSetupPolicy"
    FD_AREA  = "FV_MAIN"
    INFComponent  = "AmiModulePkg.TcgPlatformSetupPolicy"
#    Apriori_Index  = "6"
End

