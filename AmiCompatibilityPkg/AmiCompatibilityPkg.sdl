TOKEN
	Name  = "AmiCompatibilityPkg_SUPPORT"
	Value  = "1"
	Help  = "Main switch to enable AmiCompatibilityPkg support in Project"
	TokenType = Boolean
	TargetEQU = Yes
	TargetMAK = Yes
	Master = Yes
End

TOKEN
	Name  = "AMI_COMPATIBILITY_PKG_VERSION"
	Value  = "29"
	Help  = "AmiCompatibilityPkg version.\Do not change."
	TokenType = Integer
	TargetMAK = Yes
	TargetH = Yes
	Lock = Yes
End

