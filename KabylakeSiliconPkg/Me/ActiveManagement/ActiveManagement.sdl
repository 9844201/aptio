TOKEN
	Name  = "iAMT_SUPPORT"
	Value  = "1"
	Help  = "Main switch to enable iAMT support in Project"
	TokenType = Boolean
	TargetEQU = Yes
	TargetMAK = Yes
	TargetH = Yes
	Master = Yes
	Token = "iME_SUPPORT" "=" "1"
End

PATH
	Name  = "AMT_DIR"
	Help  = "iAMT Driver files source directory"
End

ELINK
    Name  = "-D AMT_SUPPORT"
    Parent  = "*_*_*_CC_FLAGS"
    Type  = "BuildOptions"
    InvokeOrder = AfterParent
    Token = "iAMT_SUPPORT" "=" "1"
End
